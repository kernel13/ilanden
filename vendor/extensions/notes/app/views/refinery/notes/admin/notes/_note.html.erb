<li class='clearfix record <%= cycle("on", "on-hover") %>' id="<%= dom_id(note) -%>">
  <span class='title'>
    <%= note.name %>

  </span>
  <span class='actions'>
    
    <%= link_to refinery_icon_tag("application_go.png"), refinery.notes_note_path(note),
        :title => t('.view_live_html'),
        :target => "_blank" %>
    
    <%= link_to refinery_icon_tag("application_edit.png"), refinery.edit_notes_admin_note_path(note),
         :title => t('.edit') %>
    <%= link_to refinery_icon_tag("delete.png"), refinery.notes_admin_note_path(note),
        :class => "cancel confirm-delete",
        :title => t('.delete'),
        :confirm => t('message', :scope => 'refinery.admin.delete', :title => note.name),
        :method => :delete %>
  </span>
</li>
